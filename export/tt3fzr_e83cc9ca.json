int i = 0; i < 150 && !hit; i++) {\n        float dist = scene(p);\n        hit = dist*dist < 1e-6;\n        p+=dist*cam;\n        if (distance(p,init)>5.) break;\n    }\n    vec3 n = norm(p);\n    vec3 r = reflect(cam,n);\n    //don't ask how I stumbled on this texture\n    vec3 nz = p - erot(p, vec3(1), 2.) + erot(p, vec3(1), 4.);\n    float spec = length(sin(r*3.5+sin(nz*120.)*.15)*.4+.6)/sqrt(3.);\n    spec *= smoothstep(-.3,.2,scene(p+r*.2));\n    vec3 col = vec3(.1,.1,.12)*spec + 1.5*pow(spec,8.);\n    col *= 1. + pow(Sound(iTime).x,3.0);\n    float bgdot = length(sin(cam*8.)*.4+.6)/2.;\n    vec3 bg = vec3(.1,.1,.11) * bgdot + pow(bgdot, 10.);\n    fragColor.xyz = hit ? col : bg;\n    fragColor = smoothstep(-.02,1.05,sqrt(fragColor)) * (1.- dot(uv,uv)*.5);\n}","name":"Image","description":"","type":"image"},{"inputs":[],"outputs":[],"code":"//This shader is modification of \"Neural Stanford Bunny (5 kb)\" by blackle:\n//https://shadertoy.com/view/wtVyWK (CC0 1.0 Universal)\n\n//I extended this to GLSLSound using the original paper as a reference.\n//I tried to play my friend's voice and draw my friend's face (called Boze).\n\n#define bpm 120.0\n#define pi2 6.2831\n\nfloat NNBozeVoice(float time){\n    float T = 1.39;//original audio length\n    float rate = 16000.;//original audio sampling rate\n    float range = 100.;//input range for Siren model(-100 - 100)\n    time = (time - T*.5)*range/T;\n    time *= 24000.0/rate;\n    vec4 f0_0=sin(time*vec4(7.79,-10.37,53.61,48.77)+vec4(15.57,-28.61,-21.72,-3.07));\n    vec4 f0_1=sin(time*vec4(59.61,20.55,23.62,-25.83)+vec4(26.23,49.69,-49.04,-20.81));\n    vec4 f0_2=sin(time*vec4(56.15,1.35,17.31,-12.97)+vec4(26.61,-4.14,29.20,-35.48));\n    vec4 f0_3=sin(time*vec4(-16.50,34.36,18.69,-33.34)+vec4(-38.64,-13.02,-42.98,3.84));\n    vec4 f0_4=sin(time*vec4(-10.66,-25.98,-51.20,41.23)+vec4(31.22,52.00,35.58,24.64));\n    vec4 f0_5=sin(time*vec4(-45.52,-20.43,-29.66,40.01)+vec4(-37.18,36.63,-1.26,-53.54));\n    vec4 f0_6=s